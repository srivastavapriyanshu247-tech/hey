#include<stdio.h>
#include<string.h>
int main(){
    char sub[20];
    printf("enter subjects you passed:");
    fgets(sub , sizeof(sub), stdin);
    if(strlen(sub) > 10){
        printf("you are rewarded Rs.45");
    }
    else
    {
        printf("you  are rewarded Rs.15");
    }
    return 0;
}
